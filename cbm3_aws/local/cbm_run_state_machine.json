{
    "Comment": "Task to launch CBM run task state machines",
    "StartAt": "map_tasks",
    "States": {
      "map_tasks": {
        "Type": "Map",
        "InputPath": "$.jobs",
        "ItemsPath": "$.jobs",
        "MaxConcurrency": 10,
        "Iterator":{
            "StartAt": "launch_cbm_task",
            "States": {
                "launch_cbm_task": {
                    "Type": "Task",
                    "Resource": "{{launch_cbm_task_resource_arn}}",
                    "End": "True"
                }
            }
        },
        "End": true
      }
    }
}